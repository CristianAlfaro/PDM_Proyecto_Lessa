<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/app_bar_main"
        tools:context=".Activities.MainActivity">

    <FrameLayout

            android:id="@+id/FL_manager"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteY="290dp" tools:layout_editor_absoluteX="28dp">

    </FrameLayout>
    <fragment
            android:id="@+id/fr_barmenu"
            tools:layout="@layout/fragment_menu_"
            class="com.e_sea_Ruby.e_sea_language.Fragments.Menu_Fragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            app:layout_constraintTop_toTopOf="parent" tools:layout_editor_absoluteX="26dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="@+id/FL_manager"
            app:layout_constraintVertical_bias="1.0"/>

</androidx.constraintlayout.widget.ConstraintLayout>